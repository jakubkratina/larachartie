{
	"name": "jakubkratina/larachartie",
	"description": "Lightweight PHP wrapper for Google Chart javascript library for Laravel",
	"keywords": [
		"laravel",
		"google charts",
		"google",
		"charts",
		"graphs",
		"php",
		"wrapper",
		"oop"
	],
	"autoload": {
		"psr-4": {
			"JK\\LaraChartie\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JK\\LaraChartie\\Test\\": "tests/"
		},
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"require": {
		"php": ">=5.3.0",
		"nesbot/carbon": "~1.20"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8",
		"nesbot/carbon": "~1.20",
		"laravel/laravel": "dev-develop"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Jakub Kratina",
			"email": "me@jakubkratina.cz"
		}
	],
	"minimum-stability": "dev"
}
